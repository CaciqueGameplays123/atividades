#include<stdio.h>

int main ()
{
   int A, B, C;
   printf("Insira o valor de A: \n");
   scanf("%i", &A);
   printf("Insira o valor de B: \n");
   scanf("%i", &B);
   printf("Insira o valor de C: \n");
   scanf("%i", &C);

   if (A > B && B > C)
   {
     printf("Opção 1: %i < %i < %i", C, B, A);
   }


   else if (A > C && C > B)
   {
    printf("Opção 2: %i < %i < %i", B, C, A);
    }


      else if (B > A && A > C)
   {
    printf("Opção 3: %i < %i < %i", C, A, B);
    }


     else if (B > C && C > A)
   {
    printf("Opção 4: %i < %i < %i", A, C, B);
    }


     else if (C > A && A > B)
   {
    printf("Opção 5: %i < %i < %i", B, A, C);
    }


     else if (C > B && B > A)
   {
    printf("Opção 6: %i < %i < %i", A, B, C);
    }

     else if (A == C && C == B)
   {
    printf("Opção 7: %i = %i = %i", B, C, A);
    }

     else if (A == B && B > C)
   {
    printf("Opção 8: %i < %i = %i", C, A, B);
    }

     else if (C > A && A == B)
   {
    printf("Opção 9: %i = %i < %i", B, A, C);
    }

     else if (A == C && C > B)
   {
    printf("Opção 10: %i < %i = %i", B, C, A);
    }

     else if (B > C && C == A)
   {
    printf("Opção 11: %i = %i < %i", C, A, B);
    }

     else if (C == B && B > A)
   {
    printf("Opção 12: %i < %i = %i", A, B, C);
    }

    else if (A > B && B == C)
   {
    printf("Opção 13: %i = %i < %i", C, B, A);
    }
    else
    {
     printf("Operacao invalida(em caso de bugs)");
    }

    }
